{
  "name": "caibird",
  "version": "4.0.0-rc.1",
  "description": "",
  "main": "",
  "type": "module",
  "scripts": {
    "dist:mjs": "rimraf dist/mjs && tsc --outDir dist/mjs",
    "dist:cjs": "rimraf dist/cjs && tsc --outDir dist/cjs --moduleResolution node --module commonjs --verbatimModuleSyntax false && node bin/rename2cjs.cjs",
    "dist": "npm run dist:mjs && npm run dist:cjs"
  },
  "author": "caibird",
  "license": "MIT",
  "devDependencies": {
    "@types/koa": "2.13.12",
    "@types/koa-send": "4.1.6",
    "@types/koa__router": "12.0.4",
    "@types/lodash": "4.14.202",
    "rimraf": "5.0.5",
    "typescript": "5.3.2"
  },
  "dependencies": {
    "@koa/router": "12.0.1",
    "@ladjs/koa-views": "9.0.0",
    "axios": "1.6.2",
    "form-data": "4.0.0",
    "koa": "2.14.2",
    "koa-body": "6.0.1",
    "koa-send": "5.0.1",
    "lodash": "4.17.21",
    "reflect-metadata": "0.1.13",
    "tslib": "2.6.2",
    "url-join": "5.0.0"
  },
  "files": [
    "/README.md",
    "/dist/mjs",
    "/dist/cjs"
  ],
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./client": {
      "import": "./dist/mjs/client/index.js",
      "require": "./dist/cjs/client/index.cjs"
    },
    "./*": {
      "import": "./dist/mjs/*",
      "require": "./dist/cjs/*"
    }
  }
}
